{
  "name": "castor-ui",
  "version": "0.1.3",
  "private": false,
  "main": "lib/castor-ui.umd.min.js",
  "scripts": {
    "lint": "eslint examples/ packages/ --ext .vue,.js --fix",
    "build-lib": "vue-cli-service build --target lib --name castor-ui --dest lib packages/index.js  && npm run build-theme",
    "build-theme": "gulp build --gulpfile packages/theme-default/gulpfile.js && cp-cli packages/theme-default/lib lib/theme-default",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.demo.js",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --config build/webpack.demo.js"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "sass-loader": "^8.0.2",
    "throttle-debounce": "^1.0.1",
    "vue": "^2.6.11",
    "vue-resize-directive": "^1.2.0",
    "element-ui": "^2.10.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "cp-cli": "^2.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "vue-router": "^3.1.6",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.11",
    "cross-env": "^3.1.3",
    "mini-css-extract-plugin": "^0.4.1",
    "copy-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "markdown-it": "^8.4.1",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-container": "^2.0.0",
    "style-loader": "^0.23.1",
    "transliteration": "^1.1.11",
    "highlight.js": "^9.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9",
    "not dead"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=10.19.0"
  }
}
